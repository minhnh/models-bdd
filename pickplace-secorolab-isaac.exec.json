{
    "@context": [
        "https://secorolab.github.io/metamodels/languages/python.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/bdd.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/behaviour.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/environment.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/simulation.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/distribution.json",
        {
            "var": "https://secorolab.github.io/models/acceptance-criteria/bdd/variants/pickplace-isaac/",
            "tmpl": "https://secorolab.github.io/models/acceptance-criteria/bdd/templates/",
            "lab-env": "https://secorolab.github.io/models/environments/secorolab/",
            "lab-sim": "https://secorolab.github.io/models/simulation/isaac/",
            "exec": "https://secorolab.github.io/models/execution/isaac-pickplace/"
        }
    ],
    "@graph": [
        {
            "@id": "exec:obj-table-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-table", "has-obj-model": "lab-sim:obj-thorlabs-table"
        },
        {
            "@id": "exec:obj-box-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-box", "has-obj-model": "lab-sim:obj-ycb-sugar-box"
        },
        {
            "@id": "exec:obj-mug-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-mug", "has-obj-model": "lab-sim:obj-ycb-mug"
        },
        {
            "@id": "exec:obj-bottle-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-bottle", "has-obj-model": "lab-sim:obj-ycb-mustard-bottle"
        },
        {
            "@id": "exec:obj-table-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-table", "has-obj-model": "lab-sim:obj-thorlabs-table"
        },
        {
            "@id": "exec:obj-ball-sim", "@type": "env:ModelledObject",
            "of-object": "lab-env:obj-ball", "has-obj-model": "lab-sim:isaac-sphere",
            "has-config": {
                "scale": [0.0315, 0.0315, 0.0315], "radius": 1.0, "color": [1, 0, 1]
            }
        },
        {
            "@id": "exec:pickplace-bhv-mockup", "@type": [ "bdd:BehaviourImplementation", "py:ModuleAttribute" ],
            "of-behaviour": "tmpl:bhv-pickplace",
            "py:module-name": "bdd_dsl.execution.mockup", "py:attribute-name": "PickplaceBehaviourMockup"
        },
        {
            "@id": "exec:pickplace-exec", "@type": [ "ScenarioExecution", "SimulatedExecution" ],
            "of-variant": "var:scr-var-panda-pickplace",
            "has-behaviour-impl": "exec:pickplace-bhv-mockup"
        }
    ]
}
