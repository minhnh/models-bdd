        },
        {
            "@id": "exec:pickplace-bhv-isaac", "@type": [ "bdd:BehaviourImplementation", "py:ModuleAttribute" ],
            "of-behaviour": "tmpl:bhv-pickplace",
            "py:module-name": "bdd_isaacsim_exec.execution", "py:attribute-name": "IsaacsimPickPlaceBehaviour"
        },
        {
            "@id": "exec:pickplace-exec", "@type": [ "bdd:ScenarioExecution", "bdd:SimulatedExecution" ],
            "of-variant": [
                "var:scr-var-panda-pickplace", "var:scr-var-panda-sorting"
            ],
            "has-behaviour-impl": "exec:pickplace-bhv-isaac"
        }
