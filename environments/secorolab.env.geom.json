{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/geometry/structural-entities.json",
        "https://secorolab.github.io/metamodels/acceptance-criteria/bdd/environment.json",
        {
            "lab": "https://secorolab.github.io/models/environments/secorolab/"
        }
    ],
    "@graph": [
        { "@id": "lab:box-origin", "@type": "Point" },
        { "@id": "lab:box-com", "@type": "Point" },
        {
            "@id": "lab:box-frame", "@type": "Frame",
            "origin": "lab:box-origin"
        },
        {
            "@id": "lab:box-body", "@type": [ "RigidBody", "SimplicialComplex", "RigidObject" ],
            "of-object": "lab:obj-box",
            "simplices": [
                "lab:box-origin",
                "lab:box-frame",
                "lab:box-com"
            ]
        },

        { "@id": "lab:ball-origin", "@type": "Point" },
        { "@id": "lab:ball-com", "@type": "Point" },
        {
            "@id": "lab:ball-frame", "@type": "Frame",
            "origin": "lab:ball-origin"
        },
        {
            "@id": "lab:ball-body", "@type": [ "RigidBody", "SimplicialComplex", "RigidObject" ],
            "of-object": "lab:obj-ball1",
            "simplices": [
                "lab:ball-origin",
                "lab:ball-frame",
                "lab:ball-com"
            ]
        },

        { "@id": "lab:bottle-origin", "@type": "Point" },
        { "@id": "lab:bottle-com", "@type": "Point" },
        {
            "@id": "lab:bottle-frame", "@type": "Frame",
            "origin": "lab:bottle-origin"
        },
        {
            "@id": "lab:bottle-body", "@type": [ "RigidBody", "SimplicialComplex", "RigidObject" ],
            "of-object": "lab:obj-bottle",
            "simplices": [
                "lab:bottle-origin",
                "lab:bottle-frame",
                "lab:bottle-com"
            ]
        },

        { "@id": "lab:dining-table-origin", "@type": "Point" },
        { "@id": "lab:dining-table-com", "@type": "Point" },
        {
            "@id": "lab:dining-table-frame", "@type": "Frame",
            "origin": "lab:dining-table-origin"
        },
        {
            "@id": "lab:dining-table-body", "@type": [ "RigidBody", "SimplicialComplex", "RigidObject" ],
            "of-object": "lab:obj-dining-table",
            "simplices": [
                "lab:dining-table-origin",
                "lab:dining-table-frame",
                "lab:dining-table-com"
            ]
        }
    ]
}
